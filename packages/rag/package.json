{
  "name": "@vibe-agent-toolkit/rag",
  "version": "0.1.1",
  "type": "module",
  "description": "Abstract RAG (Retrieval-Augmented Generation) interfaces and shared implementations",
  "keywords": [
    "rag",
    "vector-search",
    "embeddings",
    "retrieval"
