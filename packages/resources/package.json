{
  "name": "@vibe-agent-toolkit/resources",
  "version": "0.1.11",
  "description": "Markdown resource parsing, validation, and link integrity checking",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "keywords": [
    "markdown",
    "validation",
    "links",
    "resources",
    "agent-toolkit"
  ],
  "author": "Jeff Dutton",
  "license": "MIT",
  "dependencies": {
    "@vibe-agent-toolkit/utils": "workspace:*",
    "ajv": "^8.17.1",
    "js-yaml": "^4.1.1",
    "picomatch": "^4.0.3",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.10.5",
    "@types/picomatch": "^4.0.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdutton/vibe-agent-toolkit.git"
  }
}
