{
  "name": "@vibe-agent-toolkit/agent-schema",
  "version": "0.1.0-rc.1",
  "description": "JSON Schema definitions and TypeScript types for VAT agent manifest format",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schemas/*": "./schemas/*"
  },
  "files": [
    "dist/",
    "schemas/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && bun run generate:schemas",
    "generate:schemas": "tsx scripts/generate-json-schemas.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo schemas/"
  },
  "keywords": [
    "vat",
    "agent",
    "schema",
    "json-schema",
    "validation"
  ],
  "dependencies": {
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@vibe-agent-toolkit/utils": "0.1.0-rc.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
