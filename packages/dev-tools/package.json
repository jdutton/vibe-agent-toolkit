{
  "name": "@vibe-agent-toolkit/dev-tools",
  "version": "0.1.6-rc.1",
  "private": true,
  "type": "module",
  "description": "Development tools for the monorepo",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist *.tsbuildinfo",
    "prepare-bin": "tsx src/prepare-bin.ts",
    "generate-cli-docs": "tsx src/generate-cli-docs.ts",
    "validate-structure": "tsx src/validate-repo-structure.ts",
    "process-images": "tsx src/process-test-images.ts"
  },
  "dependencies": {
    "@vibe-agent-toolkit/utils": "workspace:*",
    "semver": "^7.7.3",
    "which": "^5.0.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/semver": "^7.5.0",
    "@types/which": "^3.0.4",
    "exif-reader": "^2.0.3",
    "rimraf": "^6.0.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdutton/vibe-agent-toolkit.git"
  }
}
