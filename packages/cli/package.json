{
  "name": "@vibe-agent-toolkit/cli",
  "version": "0.1.9",
  "description": "Command-line interface for vibe-agent-toolkit",
  "type": "module",
  "bin": {
    "vat": "./dist/bin/vat.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "scripts": {
    "build": "tsx src/utils/validate-help-files.ts && tsc && tsx ../dev-tools/src/prepare-bin.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config ../../vitest.integration.config.ts",
    "test:system": "vitest run --config ../../vitest.system.config.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "keywords": [
    "cli",
    "command-line",
    "agent-toolkit",
    "markdown",
    "validation"
  ],
  "author": "Jeff Dutton",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@vibe-agent-toolkit/agent-config": "workspace:*",
    "@vibe-agent-toolkit/agent-schema": "workspace:*",
    "@vibe-agent-toolkit/discovery": "workspace:*",
    "@vibe-agent-toolkit/gateway-mcp": "workspace:*",
    "@vibe-agent-toolkit/rag": "workspace:*",
    "@vibe-agent-toolkit/rag-lancedb": "workspace:*",
    "@vibe-agent-toolkit/resources": "workspace:*",
    "@vibe-agent-toolkit/agent-skills": "workspace:*",
    "@vibe-agent-toolkit/utils": "workspace:*",
    "@vibe-agent-toolkit/vat-example-cat-agents": "workspace:*",
    "commander": "^12.1.0",
    "js-yaml": "^4.1.0",
    "semver": "^7.7.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.5",
    "@types/semver": "^7.7.1",
    "rimraf": "^6.0.1",
    "tar": "^7.5.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdutton/vibe-agent-toolkit.git"
  }
}
