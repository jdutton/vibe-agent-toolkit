{
  "name": "@vibe-agent-toolkit/agent-runtime",
  "version": "0.1.1",
  "type": "module",
  "description": "Runtime framework for building and executing portable AI agents",
  "keywords": [
    "agent",
    "runtime",
    "ai",
    "llm",
    "portable",
    "toolkit"
  ],
  "author": "Jeff Dutton",
